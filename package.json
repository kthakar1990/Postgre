{
  "name": "3pm-appdirect-nestor",
  "version": "0.0.2",
  "description": "Handles Appdirect requests and sends translated request to Nestor",
  "repository": {},
  "author": "Kandarp Thakar",
  "private": true,
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules/ && rm -f package-lock.json",
    "populateDb": "node --require source-map-support/register $NODE_DEBUG_OPTION dist/cli/populatedbCli",
    "compile": "rm -rf dist && tsc && npm run tslint && npm run copy-static-assets",
    "serve": "npm run copy-static-assets && node dist/server.js",
    "dev": "npm run copy-static-assets && ./node_modules/nodemon/bin/nodemon.js",
    "watch-node": "nodemon dist/server.js",
    "test": "jest",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "start": "npm run compile && npm run serve",
    "serve-debug": "nodemon --inspect dist/server.js"
  },
  "dependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/fs-extra": "^8.0.0",
    "@types/graphql": "^14.2.3",
    "@types/jquery": "^3.3.29",
    "@types/mongodb": "^3.1.22",
    "@types/uuid": "^3.4.4",
    "async": "^2.6.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.20.1",
    "cfenv": "^1.2.2",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.1",
    "crypto-js": "^3.1.9-1",
    "deep-copy": "^1.4.2",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.18.2",
    "express-flash": "0.0.2",
    "express-graphql": "^0.9.0",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.4.2",
    "humanize-duration": "^3.20.0",
    "lusca": "^1.5.2",
    "mongodb": "2.2.33",
    "mongodb-lock": "*",
    "pg": "^7.11.0",
    "promise-prototype": "^0.1.0",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/connect-mongo": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jest": "*",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.50",
    "@types/request": "^2.47.0",
    "@types/shelljs": "^0.7.8",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "*",
    "nodemon": "^1.18.10",
    "shelljs": "^0.8.1",
    "ts-jest": "*",
    "ts-node": "*",
    "tslint": "^5.9.1",
    "typescript": "^3.2.4"
  }
